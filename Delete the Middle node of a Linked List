class Solution {
    public ListNode deleteMiddle(ListNode head) {
        if(head.next==null  || head==null)
          return null;
        ListNode slow = head;
        ListNode fast = head;
        ListNode prevNode = null;

        while(fast!= null && fast.next!= null ){
            prevNode = slow;
            slow = slow.next;
            fast = fast.next.next;
        }
        prevNode.next = slow.next;
        slow.next = null;
        return head;
          
    }
}
